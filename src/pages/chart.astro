<html>
  <head>
    <script src="https://cdn.plot.ly/plotly-2.32.0.min.js" charset="utf-8"
    ></script>
    <script>
      import Plotly from "plotly.js-dist-min";
  
      const TESTER = document.getElementById("tester")!;
      Plotly.newPlot(
        TESTER,
        [
          {
            x: [1, 2, 3, 4, 5],
            y: [1, 2, 4, 8, 16],
